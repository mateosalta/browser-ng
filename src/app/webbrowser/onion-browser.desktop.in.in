[Desktop Entry]
Version=1.0
_Name=Onion Browser
_GenericName=Onion Web Browser
_Comment=Browse the World Wide Web using QtWebEngine
_Keywords=Internet;WWW;Browser;Web;Explorer
Type=Application
Icon=@CMAKE_INSTALL_FULL_DATADIR@/morph-browser/onion-browser.png
Exec=onion-browser %u
Terminal=false
Categories=Network;WebBrowser;
MimeType=text/html;text/xml;application/xhtml+xml;x-scheme-handler/http;x-scheme-handler/https;
X-Ubuntu-Touch=true
X-Ubuntu-Gettext-Domain=morph-browser
X-Ubuntu-Single-Instance=true
X-Ubuntu-Default-Department-ID=web-browsers
X-Screenshot=@CMAKE_INSTALL_FULL_DATADIR@/morph-browser/screenshot.png
X-Ubuntu-Splash-Color=#FFFFFF
Actions=new-window;Incognito;

[Desktop Action new-window]
_Name=New Window
Exec=onion-browser --new-window

[Desktop Action Incognito]
_Name=New Private Window
Exec=onion-browser --incognito
