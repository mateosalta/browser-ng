{
    "name": "onion.nanuc.org",
    "description": "A Browser clone using tor and QtWebEngine",
    "framework": "ubuntu-sdk-16.04",
    "architecture": "@CLICK_ARCH@",
    "title": "Tor Browser",
    "icon": "@CMAKE_INSTALL_DATADIR@/morph-browser/onion-browser.png",
    "hooks": {
        "onion": {
            "apparmor": "onion-browser.apparmor",
            "desktop": "@CMAKE_INSTALL_DATADIR@/applications/onion-browser.desktop",
            "urls": "@CMAKE_INSTALL_DATADIR@/url-dispatcher/urls/onion-browser.url-dispatcher"
        }
    },
    "version": "0.3",
    "maintainer": "Aaron Kimmig <aaron@nanu-c.org>"
}
